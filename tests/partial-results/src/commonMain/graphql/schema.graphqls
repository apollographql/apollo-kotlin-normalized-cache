type Query {
  me: User!
  users(ids: [ID!]!): [User]!
  project(id: ID! = "1"): Project
  user(category: Category!): User!
}

type User {
  id: ID!
  firstName: String!
  lastName: String!
  nickName: String
  email: String!
  bestFriend: User
  projects: [Project!]!
  mainProject: Project!
  category: Category!
}

type Project {
  id: ID!
  name: String!
  description: String
  lead: User
  users: [User!]!
}

scalar Category

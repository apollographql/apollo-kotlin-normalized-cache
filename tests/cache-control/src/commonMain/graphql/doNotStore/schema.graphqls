type Query {
  user: User
}

type User {
  id: ID!
  name: String!
  email: String!
  sensitiveScalar: String
  sensitiveObject: SensitiveObject
  project: Project
}

type SensitiveObject{
  password: String
}

interface SensitiveInterface {
  topSecret: Boolean
}

type Project implements SensitiveInterface {
  id: ID!
  name: String!
  topSecret: Boolean
}
